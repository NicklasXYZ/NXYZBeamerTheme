\definecolor{light-light-gray}{RGB}{144, 144, 144}
\definecolor{gray}{RGB}{67, 67, 67}

\setbeamercolor*{background canvas}{bg=white}
\setbeamercolor*{title}{fg=black}
\setbeamerfont{title}{size=\Huge}
\setbeamerfont{subtitle}{size=\large}
\setbeamertemplate{navigation symbols}{}

\definecolor{NicklasXYZ-blue}{RGB}{41, 170, 225}
\definecolor{NicklasXYZ-green}{RGB}{12, 170, 93}

\setbeamercolor{frametitle-left}{fg=NicklasXYZ-green!75!white}
\setbeamercolor{frametitle}{fg=black}
\setbeamerfont{frametitle}{size=\Large}
\setbeamerfont{framesubtitle}{size=\small}

\setbeamerfont{block title}{size=\normalsize}
\setbeamerfont{itemize/enumerate body}{}
\setbeamerfont{itemize/enumerate subbody}{size=\scriptsize}
\setbeamerfont{itemize/enumerate subsubbody}{size=\scriptsize}

\definecolor{light-grey}{RGB}{112, 112, 112}
\setbeamercolor*{itemize item}{fg=light-grey}
\setbeamercolor*{itemize subitem}{fg=light-grey}
\setbeamercolor*{normal text}{fg=black}

\setbeamercolor*{page number in head/foot}{fg=white}
\setbeamercolor*{author in head/foot}{fg=white}
\setbeamercolor*{date in head/foot}{fg=white}

\setbeamercolor{block title}{fg=white, bg=NicklasXYZ-green!75!white}
\setbeamercolor{block body}{parent=normal text,bg=light-light-gray!10!white, fg=black}

\setbeamercolor{block title alerted}{fg=white, bg=light-light-gray!75!white}
\setbeamercolor{block body alerted}{parent=normal text, bg=light-light-gray!10!white, fg=black}

\setbeamercolor{block title example}{fg=white, bg=NicklasXYZ-blue}
\setbeamercolor{block body example}{parent=normal text,bg=light-light-gray!10!white, fg=black}
