% Define colors
\definecolor{base-color-one}{RGB}{114, 17, 33}
\definecolor{base-color-two}{RGB}{114, 17, 33}
\definecolor{base-color-three}{RGB}{114, 17, 33}
\definecolor{base-color-four}{RGB}{114, 17, 33}
\definecolor{base-color-five}{RGB}{114, 17, 33}

% Set beamer element colors
\setbeamercolor{frametitle-left}{fg=base-color-four!85!white}
\setbeamercolor{frametitle}{fg=black}

% \setbeamercolor{itemize item}{fg=white}
\setbeamercolor*{itemize item}{fg=base-color-five}
\setbeamercolor*{itemize subitem}{fg=base-color-five}

\setbeamercolor{enumerate item}{fg=base-color-four!85!white}
\setbeamercolor{description item}{fg=white}

\setbeamercolor{block title}{fg=white, bg=base-color-four!85!white}
\setbeamercolor{block body}{parent=normal text,bg=base-color-one!10!white, fg=black}

\setbeamercolor{block title alerted}{fg=white, bg=base-color-one!85!white}
\setbeamercolor{block body alerted}{parent=normal text, bg=base-color-one!10!white, fg=black}

\setbeamercolor{block title example}{fg=white, bg=base-color-three}
\setbeamercolor{block body example}{parent=normal text,bg=base-color-one!10!white, fg=black}

\setbeamercolor{section in toc}{fg=black}
\setbeamercolor{subsection in toc}{fg=black}
\setbeamercolor{section number projected}{bg=base-color-four!85!white, fg=white}
\setbeamercolor{subsection number projected}{bg=base-color-five, fg=white}

% Misc.
\setbeamercolor*{title}{fg=black}
\setbeamercolor*{normal text}{fg=black}
\setbeamercolor*{background canvas}{bg=white}
\setbeamercolor*{page number in head/foot}{fg=white}
\setbeamercolor*{author in head/foot}{fg=white}
\setbeamercolor*{date in head/foot}{fg=white}